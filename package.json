{
  "name": "britetestchallenge",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "pretest": "del /Q .\\cypress\\report\\* && del /Q .\\cypress\\report\\mochawesome-report\\* ",
    "test": "npm run e2e:firefox && npm run e2e:chrome",
    "e2e:firefox":"cypress run --browser firefox",
    "e2e:chrome":"cypress run --spec cypress/e2e/actorUpcomingTest.cy.js --browser chrome",
    "posttest": "npm run merge-reports && npm run generate-htmlreport",
    "merge-reports": "mochawesome-merge cypress/report/mochawesome-report/mochawesome*.json > cypress/report/britetest.json",
    "generate-htmlreport": "marge cypress/report/britetest.json --reportDir ./ --inline"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "chai-json-schema": "^1.5.1",
    "cypress": "^13.13.1"
  },
  "devDependencies": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
